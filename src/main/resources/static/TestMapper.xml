<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ml.mapper.TestMapper">


    <insert id="insertUser">
        insert into t_user (id,name,age,sex)
            VALUES (#{id},#{name},#{age},#{sex,javaType=com.ml.enums.SexEnum})
    </insert>


    <select id="selectUserList" resultType="com.ml.domain.UserModel">
        select id,name,age,sex from t_user
    </select>
</mapper>
